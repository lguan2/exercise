    public static int[] productExceptSelf(int[] nums) {

        int len = nums.length;
        int[] res = new int[len];
        res[len - 1] = 1;
        for (int i = len - 2; i >= 0; i--) {
            res[i] = nums[i + 1] * res[i + 1];
        }
        int left = 1;
        for (int i = 1; i < len; i++) {
            res[i] *= left;
            left *= nums[i];
        }
        return res;
    }
