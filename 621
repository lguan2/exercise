    public int leastInterval(char[] tasks, int n) {
        char[] count = new char[26];
        int max = 0;
        int dupMax = 0;
        for (char task : tasks) {
            count[task - 'A']++;
            max = Math.max(max, count[task - 'A']);
        }
        for (int i = 0; i < count.length; i++) {
            if (count[i] == max) dupMax++;
        }
        // compare tasks.length? max can be one?
        return (max - 1) * (n + 1) + dupMax;
    }
