    // int[] nums = {0,0}
    // int[] nums = {1,0,0}
    // int[] nums = {0,0,1}
    // int[] nums = {1,0,2,0,2}
    public void moveZeros(int[] nums) {
        if (nums == null || nums.length <= 1) return;
        int right = nums.length;
        int p = 0;
        while (p < right) {
            if (nums[p] == 0) {
                nums[p] = nums[--right];
                nums[right] = 0;
            } else {
                p++;
            }
        }
    }
