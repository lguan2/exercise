    public boolean validCapital(String s) {
        if(s==null || s.length() <=1)
            return true;
        if (s.toLowerCase().equals(s) || s.toUpperCase().equals(s)) return true;
        for (int i = 1; i < s.length(); i++) {
            if (s.charAt(i) < 'a' || s.charAt(i) > 'z') {
                return false;
            }
        }
        return true;
    }
    
